<Product specieID="pLIMK"/>
<Product specieID="RacPAK"/>

<forwardRate>0.168e-3</forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>

<!--LIMK activated by PKAc-->
<!--PKAc+LIMK <->pLIMK+PKAc-->
<Reaction id="PKAc+LIMK--LIMKPKAc" name="PKAc+LIMK--LIMKPKAc">
<Reactant specieID="PKAc"/>
<Reactant specieID="LIMK"/>
<Product specieID="PKAcLIMK"/>

<forwardRate>0.6e-6</forwardRate>
<reverseRate>2e-3</reverseRate>

</Reaction>

<Reaction id="LIMKPKAc--PKAc+pLIMK" name="LIMKPKAc--PKAc+pLIMK">
<Reactant specieID="PKAcLIMK"/>
<Product specieID="pLIMK"/>
<Product specieID="PKAc"/>

<forwardRate>0.50e-3</forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>

