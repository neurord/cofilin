<!--****************************************************************************
Now, the reactions themselves
****************************************************************************-->
<!--activation of Kal-->
<!--CamKII to Kal -->
<!--CamKII+Kal<->CamKII +pKal-->
<!--part 1-->
<Reaction id="Kal+CKCamCa4--KalCKCamCa4" name="Kal+CKCamCa4--KalCKCamCa4">
<Reactant specieID="Kal"/>
<Reactant specieID="CKCamCa4"/>
<Product specieID="KalCKCamCa4"/>

<forwardRate> 0.000005e-3 </forwardRate>
<reverseRate> 0.04e-3 </reverseRate>


</Reaction>
<Reaction id="KalCKCamCa4--pKal+CKCamCa4" name="KalCKCamCa4--pKal+CKCamCa4">
<Reactant specieID="KalCKCamCa4"/>
<Product specieID="CKCamCa4"/>
<Product specieID="pKal"/>

<forwardRate> 0.01e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>

<!--part 2-->
<!--CamKII+Kal<->CamKII +pKal-->
<Reaction id="Kal+CKpCamCa4--KalCKpCamCa4" name="Kal+CKpCamCa4--KalCKpCamCa4">
<Reactant specieID="Kal"/>
<Reactant specieID="CKpCamCa4"/>
<Product specieID="KalCKpCamCa4"/>

<forwardRate>0.000050e-3</forwardRate>
<reverseRate>0.4e-3</reverseRate>


</Reaction>
<Reaction id="KalCKpCamCa4--pKal+CKpCamCa4" name="KalCKpCamCa4--pKal+CKpCamCa4">
<Reactant specieID="KalCKpCamCa4"/>
<Product specieID="CKpCamCa4"/>
<Product specieID="pKal"/>

<forwardRate>0.1e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>


<!--PKA to Kal -->
<!--PKAc+Kal<->PKAc +pKal-->
<Reaction id="Kal+PKAc--KalPKAc" name="Kal+PKAc--KalPKAc">
<Reactant specieID="Kal"/>
<Reactant specieID="PKAc"/>
<Product specieID="KalPKAc"/>

<forwardRate>0.00030e-3</forwardRate>
<reverseRate> 2.4e-3</reverseRate>


</Reaction>
<Reaction id="KalPKAc--pKal+PKAc" name="KalPKAc--pKal+PKAc">
<Reactant specieID="KalPKAc"/>
<Product specieID="PKAc"/>
<Product specieID="pKal"/>

<forwardRate>0.6e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>


<!--PP1 inactivates Kal -->
<!--PP1+Kal<->PP1 +pKal-->
<Reaction id="pKal+PP1--pKalPP1" name="pKal+PP1--pKalPP1">
<Reactant specieID="pKal"/>
<Reactant specieID="PP1"/>
<Product specieID="pKalPP1"/>

<forwardRate> 0.00005e-3 </forwardRate>
<reverseRate> 0.04e-3 </reverseRate>

</Reaction>
<Reaction id="pKalPP1--Kal+PP1" name="pKalPP1--Kal+PP1">
<Reactant specieID="pKalPP1"/>
<Product specieID="PP1"/>
<Product specieID="Kal"/>

<forwardRate> 0.01e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>



<!--RacGDP bind to 1433-->
<!--RacGDP+ RCap<->RCapGDP-->
<Reaction id="RacGDP+RCap--RCapGDP" name="RacGDP+RCap--RCapGDP">
<Reactant specieID="RCap"/>
<Reactant specieID="RacGDP"/>
<Product specieID="RCapGDP"/>

<forwardRate>0.1e-3</forwardRate>
<reverseRate>0</reverseRate>

</Reaction>

<!--RacGTP activation CHANGED-->
<!--RacGDP+ pKal<->RacGTP + pKal-->
<Reaction id="RacGDP+Kal--pKalRacGDP" name="RacGDP+pKal--pKalRacGDP">
<Reactant specieID="pKal"/>
<Reactant specieID="RCapGDP"/>
<Product specieID="pKalRac"/>

<forwardRate>0.000375e-3</forwardRate>
<reverseRate>0.3e-3</reverseRate>

</Reaction>

<Reaction id="pKalRacGDP--pKal+RacGTP" name="pKalRacGDP--pKal+RacGTP">
<Reactant specieID="pKalRac"/>
<Product specieID="RacGTP"/>
<Product specieID="RCap"/>
<Product specieID="pKal"/>

<forwardRate>1.5e-3</forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>


<!--RacGTP inactivation -->
<!--RacGTP+ Gap<->RacGDP + Gap-->
<Reaction id="RacGTP+Gap--GapRacGTP" name="RacGTP+Gap--GapRacGTP">
<Reactant specieID="Gap"/>
<Reactant specieID="RacGTP"/>
<Product specieID="RacGap"/>

<forwardRate> 2.2e-6 </forwardRate>
<reverseRate> 4.82e-3</reverseRate>

</Reaction>

<Reaction id="GapRacGTP--RacGDP+Gap" name="GapRacGTP--RacGDP+Gap">
<Reactant specieID="RacGap"/>
<Product specieID="RacGDP"/>
<Product specieID="Gap"/>

<forwardRate> 1.21e-3 </forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>

<!--RacGTP activate PAK-->
<!--RacGTP+PAK <->RacPAK-->
<Reaction id="RacGTP+PAK--RacGTPPAK" name="RacGTP+PAK--RacGTPPAK">
<Reactant specieID="PAK"/>
<Reactant specieID="RacGTP"/>
<Product specieID="RacPAK"/>

<forwardRate>0.009432854292459061e-6</forwardRate>
<reverseRate>0.001639e-3</reverseRate>

</Reaction>

<!-- intrinsic inactivation of PAK CHANGED-->
<!--RacPAK<->RacGDP+PAK-->
<Reaction id="RacPAK--RacGDP+PAK" name="RacPAK--RacGDP+PAK">
<Reactant specieID="RacPAK"/>
<Product specieID="RacGDP"/>
<Product specieID="PAK"/>

<forwardRate>4.6764115604291607e-8</forwardRate>
<reverseRate>0</reverseRate>

</Reaction>

<!--pSSH bind to 1433-->
<!--pSSH+ SCap<->SCapGDP-->
<Reaction id="pSSH+SCap--SCappSSH" name="pSSH+SCap--SCappSSH">
<Reactant specieID="SCap"/>
<Reactant specieID="pSSH"/>
<Product specieID="SCappSSH"/>

<forwardRate>0.01e-3</forwardRate>
<reverseRate>0</reverseRate>

</Reaction>

<!-- CaN activate SSH-->
<!--  CaNCam+pSSH <->CaNCam+SSH       -->
<Reaction name="CaNCamCa4+pSSH--CaNCamCa4pSSH" id="CaNCamCa4+pSSH--CaNCamCa4pSSH">
        <Reactant specieID="SCappSSH"/>
        <Reactant specieID="CaNCamCa4"/>
        <Product specieID="CaNCamCa4pSSH"/>

        <!--forwardRate>0.336e-06</forwardRate-->
        <!--reverseRate>1.344e-3</reverseRate-->
	<forwardRate>0.48e-06</forwardRate>
        <reverseRate>1.92e-3</reverseRate>

</Reaction>

<!--  CaNCam+pSSH <->CaNCam+SSH -->
<Reaction name="CaNCamCa4pSSH--SSH+CaNCamCa4" id="CaNCamCa4pSSH--SSH+CaNCamCa4">
        <Reactant specieID="CaNCamCa4pSSH"/>
        <Product specieID="CaNCamCa4"/>
        <Product specieID="SSH"/>
	<Product specieID="SCap"/>
	
       	<!--forwardRate>0.336e-3</forwardRate-->
	<forwardRate>0.48e-3</forwardRate>
        <reverseRate> 0 </reverseRate>
</Reaction>

<!--PAK inactivate SSH through phosphorylation-->
<Reaction id="RacPAK+SSH--SSHRacPAK" name="RacPAK+SSH--SSHRacPAK">
<Reactant specieID="RacPAK"/>
<Reactant specieID="SSH"/>
<Product specieID="RacPAKSSH"/>


<forwardRate>0.1e-6</forwardRate>
<reverseRate>0.33e-3</reverseRate>
</Reaction>

<Reaction id="SSHRacPAK--pSSH+RacPAK" name="SSHRacPAK--pSSH+RacPAK">
<Reactant specieID="RacPAKSSH"/>
<Product specieID="pSSH"/>
<Product specieID="RacPAK"/>

<forwardRate>0.083e-3</forwardRate>
<reverseRate> 0 </reverseRate>

</Reaction>

<!--LIMK activated by PAK-->
<!--PAK+LIMK <->pLIMK+PAK-->
<Reaction id="RacPAK+LIMK--LIMKRacPAK" name="RacPAK+LIMK--LIMKRacPAK">
<Reactant specieID="RacPAK"/>
<Reactant specieID="LIMK"/>
<Product specieID="RacPAKLIMK"/>

<forwardRate>0.2e-6</forwardRate>
<reverseRate>0.68e-3</reverseRate>

</Reaction>

<Reaction id="LIMKRacPAK--RacPAK+pLIMK" name="LIMKRacPAK--RacPAK+pLIMK">
